<div style="padding: 20px; display: flex; justify-content:center; align-content:
    center;">
    <button type="button" class="btn btn-primary m-4"><a href="/links/proceso1"
            class="btn btn-primary">Iniciar proceso</a></button>
</div>
<div class="container p-4">
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Material</th>
            <th scope="col">Cantidad de Material</th>
            <th scope="col">Cantidad con residuos</th>
            <th scope="col">Tiempo(min)</th>
        </tr>
    </thead>
    <tbody>
        {{#each registros}}
        <tr>
            <th scope="row">{{id}}</th>
            <td>{{material}}</td>
            <td>{{cantidad_material}}</td>
            <td>{{residuos}}</td>
            <td>{{tiempo}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
</div>